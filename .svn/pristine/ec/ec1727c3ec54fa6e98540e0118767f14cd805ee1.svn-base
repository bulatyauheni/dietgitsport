<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="50dip"
    android:cacheColorHint="#00000000"
    android:orientation="horizontal" >

    <LinearLayout
        android:id="@+id/linearLayoutDishWeight"
        android:layout_width="0px"
        android:layout_height="fill_parent"
        android:layout_weight="5"
        android:orientation="vertical" >
<LinearLayout
            android:id="@+id/linearLayoutDishCall"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="2"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/textViewDishName"
                android:layout_width="wrap_content"
                android:layout_height="fill_parent"
                android:gravity="center_vertical"
                android:layout_marginLeft="10dip"
                android:maxLines="3"
                android:text="TextView"
                android:textColor="@color/text_color"
                android:textSize="14dip"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/textViewDishType"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="3dp"
                android:text="@string/portion_size"
                android:textColor="@color/text_color"
                android:textSize="19dip"
                android:visibility="gone" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linearLayoutDishCall"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <LinearLayout
                android:id="@+id/linearLayoutDishCall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

               <TextView
                    android:id="@+id/textViewCaloricityPre"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center_vertical"
                    android:layout_marginLeft="10dip"
                    android:maxLines="2"
                    android:text="@string/energy"
                    android:textColor="@color/text_color"
                    android:textSize="10dip" />
                 <TextView
                    android:id="@+id/textViewCaloricity"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text=""
                    android:textColor="@color/text_color"
                    android:textSize="10dip" />
				 <TextView
                    android:id="@+id/textViewCaloricityLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="калл"
                    android:textColor="@color/text_color"
                    android:textSize="10dip" />
               
            </LinearLayout>
 <LinearLayout
                android:id="@+id/linearLayoutDishCall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textViewProteinLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="6dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="@string/protein"
                    android:textColor="@color/protein"
                    android:textSize="10dip" />

                <TextView
                    android:id="@+id/textViewProtein"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="4dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text=""
                    android:textColor="@color/protein"
                    android:textSize="10dip" />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/linearLayoutDishCall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textViewFatLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="7dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="@string/fat"
                    android:textColor="@color/fat"
                    android:textSize="10dip" />

                <TextView
                    android:id="@+id/textViewFat"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text=""
                    android:textColor="@color/fat"
                    android:textSize="10dip" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/linearLayoutDishCall"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/textViewCarbonLabel"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="7dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text="@string/carbon"
                    android:textColor="@color/carbon"
                    android:textSize="10dip" />

                <TextView
                    android:id="@+id/textViewCarbon"
                    android:layout_width="wrap_content"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5dip"
                    android:gravity="center_vertical"
                    android:maxLines="2"
                    android:text=""
                    android:textColor="@color/carbon"
                    android:textSize="10dip" />
            </LinearLayout>

           
        </LinearLayout>

        
    </LinearLayout>

    <LinearLayout
        android:layout_width="0px"
        android:layout_height="fill_parent"
        android:layout_weight="0.9"
        android:gravity="right"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/textViewId"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent"
            android:textSize="15dip"
            android:textStyle="bold"
            android:visibility="gone" />

        <Button
            android:id="@+id/buttonDell"
            android:layout_width="35dip"
            android:layout_height="35dip"
            android:layout_margin="5dip"
            android:background="@drawable/dell_button_xml"
            android:clickable="true"
            android:focusable="false"
            android:gravity="center_vertical"
            android:maxLines="1"
            android:textSize="12dip"
            android:visibility="gone" >
        </Button>
    </LinearLayout>

</LinearLayout>